{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"lib": [
			"esnext"
		],
		"types": [
			"@cloudflare/workers-types"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"paths": {
			"@config": [
				"./src/config"
			],
			"@api/*": [
				"./src/api/*"
			],
			"@types/*": [
				"./src/types/*"
			],
			"@schemas/*": [
				"./src/database/drizzle-orm/schemas/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
			"@helpers/*": [
				"./src/database/helpers/*"
			],
			"@lib/*": [
				"./src/lib/*"
			],
			"@handlers/*": [
				"./src/handlers/*"
			]
		}
	}
}